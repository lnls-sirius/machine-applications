[Unit]
Description=Power Supply IOC - BO Correctors - IA05
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
LimitCORE=infinity
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-bo-ps-correctors-ia05'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-bo-ps-correctors-ia05
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py IA-05RaCtrl:CO-PSCtrl-BO
