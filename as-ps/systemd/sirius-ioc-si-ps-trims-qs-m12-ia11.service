[Unit]
Description=Soft IOC for SI Trims Subsections 11M1, 11M2
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-m12-ia11'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-m12-ia11
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-11RaCtrl:CO-PSCtrl-SI1
