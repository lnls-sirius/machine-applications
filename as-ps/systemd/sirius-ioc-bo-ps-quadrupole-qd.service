[Unit]
Description=Power Supply IOC - BO Quadrupoles QD
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
LimitCORE=infinity
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-bo-ps-quadrupole-qd'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-bo-ps-quadrupole-qd
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py PA-RaPSC03:CO-PSCtrl-BO4
