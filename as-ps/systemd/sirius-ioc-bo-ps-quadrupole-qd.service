[Unit]
Description=Power Supply IOC - BO Quadrupoles QD
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-bo-ps-quadrupole-qd'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-bo-ps-quadrupole-qd
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth PA-RaPSC03:CO-PSCtrl-BO4
