[Unit]
Description=Power Supply IOC - SI Correctors Subsections 02M1, 02M2
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia02-m12'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia02-m12
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-02RaCtrl:CO-PSCtrl-SI3
