[Unit]
Description=Power Supply IOC - SI Correctors Subsections 15C2, 15M1, 15M2
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia15-c2m12'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia15-c2m12
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-15RaCtrl:CO-PSCtrl-SI2
