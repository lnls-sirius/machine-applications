[Unit]
Description=Power Supply IOC - BO Correctors - IA04
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-bo-ps-correctors-ia04'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-bo-ps-correctors-ia04
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-04RaCtrl:CO-PSCtrl-BO
