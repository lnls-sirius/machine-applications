[Unit]
Description=Power Supply IOC - BO Correctors - IA11
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-bo-ps-correctors-ia11'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-bo-ps-correctors-ia11
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py IA-11RaCtrl:CO-PSCtrl-BO
