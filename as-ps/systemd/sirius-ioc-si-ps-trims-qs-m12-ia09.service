[Unit]
Description=Soft IOC for SI Trims Subsections 09M1, 09M2
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-m12-ia09'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-m12-ia09
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-09RaCtrl:CO-PSCtrl-SI1
