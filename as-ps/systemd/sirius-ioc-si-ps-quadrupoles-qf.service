[Unit]
Description=Power Supply IOC - SI Quadrupoles QFB QFP QFA
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
LimitCORE=infinity
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-quadrupoles-qf'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-quadrupoles-qf
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py PA-RaPSA02:CO-PSCtrl-SI1
