[Unit]
Description=Power Supply IOC - TB Dipoles
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ps-dipoles'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ps-dipoles
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth LA-RaPS02:CO-PSCtrl-TB
