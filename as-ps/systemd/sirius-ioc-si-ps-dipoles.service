[Unit]
Description=Power Supply IOC - SI Dipoles
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-dipoles'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-dipoles
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py PA-RaPSD04:CO-PSCtrl-SI
