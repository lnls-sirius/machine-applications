[Unit]
Description=Power Supply IOC - SI Correctors Subsections 17C1, 17C3, 17C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia17-c134'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia17-c134
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-17RaCtrl:CO-PSCtrl-SI4
