[Unit]
Description=Power Supply IOC - SI Dipoles 1
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-dipole-1'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-dipole-1
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth PA-RaPSD04:CO-PSCtrl-SI1
