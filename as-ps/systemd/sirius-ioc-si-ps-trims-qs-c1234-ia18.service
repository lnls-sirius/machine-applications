[Unit]
Description=Soft IOC for SI Trims Subsections 18C1, 18C2, 18C3, 18C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-c1234-ia18'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-c1234-ia18
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-18RaCtrl:CO-PSCtrl-SI3
