[Unit]
Description=Soft IOC for SI Trims Subsections 07C1, 07C2, 07C3, 07C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-c1234-ia07'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-trims-qs-c1234-ia07
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-07RaCtrl:CO-PSCtrl-SI3
