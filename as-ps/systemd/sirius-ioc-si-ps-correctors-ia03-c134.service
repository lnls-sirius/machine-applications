[Unit]
Description=Power Supply IOC - SI Correctors Subsections 03C1, 03C3, 03C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia03-c134'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-correctors-ia03-c134
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth IA-03RaCtrl:CO-PSCtrl-SI4
