[Unit]
Description=Power Supply IOC - TB Correctors
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ps-correctors'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ps-correctors
ExecStart=/usr/local/bin/sirius-ioc-as-ps.py --eth LA-RaCtrl:CO-PSCtrl-TB2
