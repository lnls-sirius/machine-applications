[Unit]
Description=SI Skew Quadrupoles Power Supply Diagnostics IOC
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-si-ps-diag-qs'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-si-ps-diag-qs
ExecStart=/usr/local/bin/sirius-ioc-as-ps-diag.py SI ".*" "QS.*"
