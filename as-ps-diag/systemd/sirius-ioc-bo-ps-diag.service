[Unit]
Description=BO Power Supply Diagnostics IOC
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-bo-ps-diag'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-bo-ps-diag
ExecStart=/usr/local/bin/sirius-ioc-as-ps-diag.py BO ".*" ".*"
