[Unit]
Description=TS Power Supply Diagnostics IOC
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-ts-ps-diag'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-ts-ps-diag
ExecStart=/usr/local/bin/sirius-ioc-as-ps-diag.py TS ".*" ".*"
