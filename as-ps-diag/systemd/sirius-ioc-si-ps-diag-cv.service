[Unit]
Description=SI Vertical Correctors Power Supply Diagnostics IOC
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ps-diag-cv'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ps-diag-cv
ExecStart=/usr/local/bin/sirius-ioc-as-ps-diag.py SI ".*" "CV.*"
