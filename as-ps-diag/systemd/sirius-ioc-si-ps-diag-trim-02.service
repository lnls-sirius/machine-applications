[Unit]
Description=SI Trims Power Supply Diagnostics IOC
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-si-ps-diag-trim-02'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-si-ps-diag-trim-02
ExecStart=/usr/local/bin/sirius-ioc-as-ps-diag.py SI "[0-2][0-9].*" "Q(F|D).*"
